<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Menu</title>
</head>
<body>
    <%- include('./partials/nav') %>
    <h1>Full Menu</h1>
    <ul>
        <% menu.forEach((item) => { %>
            <li>Item: <%= item.name %></li>
            <li>Price: <%= item.price %></li>
            <li>Rating: <%= item.rating %></li>
            <li>Details: <%= item.details %></li>
            </br>
    <%    }); %>
    </ul>
</body>
</html>
